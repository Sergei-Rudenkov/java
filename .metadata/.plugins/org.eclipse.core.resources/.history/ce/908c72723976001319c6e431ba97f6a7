import java.util.*;
public class ArrayBilder extends ArrayBilderTest {
	
	Random rand = new Random();
	int calc, maxcalc;
public List<Integer> arrayMetod(){
	 List<Integer> anArray = new ArrayList<Integer>();
	for (int a = 0; a <= i; a++);{
		int  n = rand.nextInt(2);
		anArray.add(n);
		if( n == 1 ){
			calc++;
			if (maxcalc < calc){
				maxcalc = calc;
			}
		}else calc = 0;
	}	
	return anArray;
}
}
