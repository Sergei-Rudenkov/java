import java.util.ArrayList;

public class PoliMorp {

	public static void main(String[] args) {
		Student strTest = new Student();
		Bilder blrTest = new Bilder();
		ArrayList<Human> listTest = new ArrayList<Human>();
        listTest.add(strTest); 
        listTest.add(blrTest);
        for (Human x : listTest){
        	x.Live();
        	System.out.println(x.age);
        	System.out.println(x.name);
        	System.out.println(x.health);
        	}
	}
}
