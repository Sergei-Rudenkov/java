package com.epam.rudenkov;

public class Essence {
	int health = 100;
	int age = 0;

	void toDie(){
	
	}
	Essence toKill(Essence x){
		x.health = 0;
		return null;
	
	}

	void toBreath(){
	
	}
	
	Essence toHaveSex(Essence a){
		if (a.getClass() == this.getClass()){
			Essence newEs = new Essence();
			return newEs;
		}
		return null;
	}

}
