import java.util.*;
public class ArrayBilder extends ArrayBilderTest {
	List anArray = new ArrayList();
	Random rand = new Random();
	int calc, maxcalc;
public void arrayMetod(){
//	anArray = new int[i];
	for (int a = 0; a <= i; a++);{
		int  n = rand.nextInt(1);
		anArray.add(n);
		if( n == 0 ){
			calc++;
			if (maxcalc < calc){
				maxcalc = calc;
			}
		}else calc = 0;
	}	
}
}
